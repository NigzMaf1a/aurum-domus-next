    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "mysql2": "^3.14.1",
    "next": "15.3.3",
    "next-i18n-router": "^5.5.2",
    "react": "^19.0.0",
